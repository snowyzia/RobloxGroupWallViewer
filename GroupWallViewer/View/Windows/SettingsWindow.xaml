<Window x:Class="GroupWallViewer.View.Windows.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GroupWallViewer.View.Windows"
        mc:Ignorable="d"
        Title="Settings" Height="100" Width="200">
    <Grid>
        <StackPanel Margin="5">
            <CheckBox IsChecked="{Binding DisplayUserIcons}" Content="Show User Icons" Checked="DisplayUserIconsOn" Unchecked="DisplayUserIconsOff"/>
        </StackPanel>
    </Grid>
</Window>
